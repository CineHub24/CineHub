{
	"name": "cinehub",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:unit:coverage": "vitest --coverage",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"db:start": "docker compose up",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:startWithPodman": "podman compose up --detach && drizzle-kit push",
		"compilei18N": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@stripe/stripe-js": "^4.10.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/svelte": "^5.2.6",
		"@types/jest": "^29.5.14",
		"@types/node-schedule": "^2.1.7",
		"@types/nodemailer": "^6.4.17",
		"@types/qrcode": "^1.5.5",
		"@vitest/coverage-v8": "^2.1.6",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.22.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"jest": "^29.7.0",
		"jsdom": "^25.0.1",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"stripe": "^17.5.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-stripe": "^1.3.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.11.1",
		"@node-rs/argon2": "^1.1.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@paypal/paypal-js": "^8.1.2",
		"@pilcrowjs/object-parser": "^0.0.4",
		"@prisma/client": "^6.1.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.10.2",
		"@types/pdfkit": "^0.13.8",
		"arctic": "^2.3.0",
		"drizzle-orm": "^0.33.0",
		"font-awesome": "^4.7.0",
		"html5-qrcode": "^2.3.8",
		"ical-generator": "^8.0.1",
		"js-cookie": "^3.0.5",
		"jsqr": "^1.4.0",
		"lucide-svelte": "^0.464.0",
		"node-schedule": "^2.1.1",
		"nodemailer": "^6.9.16",
		"pdfkit": "^0.16.0",
		"postgres": "^3.4.4",
		"qrcode": "^1.5.4"
	}
}
